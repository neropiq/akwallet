      "outDir": "./public/",
      "sourceMap": true,
      "noImplicitAny": true,
      "module": "commonjs",   
      "resolveJsonModule": true,
      "esModuleInterop": true,
      "target": "es5",
      "lib": [
        "es6", "dom"
      ],
      "jsx": "react",
      "allowJs": true
    },
    "include": [
        "./src/**/*"
    ]
}